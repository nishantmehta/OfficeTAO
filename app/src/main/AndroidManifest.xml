<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tao"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.tao.ui.MainScreen"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.tao.ui.ListViewCustom"
            android:label="@string/title_activity_list_view_custom" >
        </activity>
        <activity
            android:name="com.tao.ui.TAOMainScreen"
            android:label="@string/title_activity_taomain_screen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tao.ui.DataFormActivity"
            android:label="@string/title_activity_data_form" >
        </activity>
    </application>

</manifest>
